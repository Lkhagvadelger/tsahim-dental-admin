{
  "name": "tsahim-dental-admin",
  "version": "0.0.0",
  "private": true,
  "prisma": {
    "seed": "npx ts-node -T prisma/seed.ts"
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "scripts": {
    "postinstall": "husky install",
    "dev": "next dev -p 3400",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen": "fx add resource",
    "prisma:migrate:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.prod -- prisma migrate deploy",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:seed": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma db seed",
    "prisma:reset": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate reset",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "heroku-postbuild": "next build && npm run prisma:migrate:prod",
    "timer:check-next-message": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env -- npx ts-node ./lib/bot/api/timer"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.7",
    "@chakra-ui/theme-tools": "^1.3.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@faker-js/faker": "^7.2.0",
    "@fontsource/inter": "^4.5.1",
    "@fontsource/playfair-display": "^4.5.1",
    "@prisma/client": "^3.3.0",
    "@quixo3/prisma-session-store": "^3.1.2",
    "@slack/web-api": "^6.8.0",
    "@stripe/stripe-js": "^1.19.1",
    "aws-sdk": "^2.1014.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cookie-signature": "^1.1.0",
    "cross-fetch": "^3.1.4",
    "csvtojson": "^2.0.10",
    "dotenv-cli": "^4.1.1",
    "draft-js": "^0.11.7",
    "express-validator": "^6.14.0",
    "framer-motion": "^4.1.17",
    "googleapis": "^103.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.40",
    "next": "^12.0.7",
    "next-connect": "^0.11.0",
    "next-session": "^4.0.4",
    "next-translate": "^1.0.7",
    "nodemailer": "^6.7.5",
    "openai": "^3.1.0",
    "passport": "^0.5.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "posthog-js": "^1.16.4",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.19.5",
    "react-icons": "^4.2.0",
    "react-query": "^3.33.1",
    "react-timeago": "^6.2.1",
    "slate-react": "^0.79.0",
    "ts-custom-error": "^3.2.0",
    "twilio": "^3.72.0"
  },
  "devDependencies": {
    "@founding/fx": "latest",
    "@svgr/cli": "^5.5.0",
    "@svgr/webpack": "^5.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-signature": "^1.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.12",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/react": "^17.0.37",
    "@types/react-timeago": "^4.1.3",
    "eslint": "^8.4.1",
    "eslint-config-next": "^12.0.7",
    "husky": "^7.0.4",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.2",
    "prisma": "^3.6.0",
    "stripe": "^8.190.0",
    "typescript": "^4.5.3"
  },
  "engines": {
    "node": "16.x"
  }
}
